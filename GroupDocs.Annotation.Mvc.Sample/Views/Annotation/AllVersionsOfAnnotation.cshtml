@{
    ViewBag.Title = "All version of annotation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model GroupDocs.Annotation.Mvc.Sample.Models.AnnotationVersionModel

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>DocumentAnnotation</title>
</head>

<body>
<nav class="navbar navbar-default">
    <div class="collapse navbar-collapse">
        <ul class="nav navbar-nav">
            <li>
                <h2>@Model.documentMetadata.Name</h2>
            </li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
            <li>
                <a href="@Url.Action("DocumentAnnotations", "Annotation", new {fileId = @Model.documentMetadata.DocumentId})">Back to the annotations</a>
            </li>
            <li>
                <a href="@Url.Action("Index", "Annotation")">Back to the main page</a>
            </li>
        </ul>
    </div>
</nav>

@foreach (var annotation in Model.annotationsDictionary)
{
    <div class="row">
        <div class="col-md-8 col-sm-offset-2">
            <textarea rows="30" style="width: 100%" readonly>
                
                
                    @annotation.Value
                
            
            </textarea>
        </div>
        <br/>
    </div>
}
</body>
</html>